<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{35} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.jamesdbloom" level="TRACE"/>

    <root level="warn">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>